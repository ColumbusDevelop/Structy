def uncompress(s):
  
  output = ""
  count = ""
  
  for character in s:
    
    if character.isnumeric() == True:
      
      count = count + character
      
    if character.isalpha() == True:
      
      for number in range(int(count)):
        output = output + character
        count = ""

  return output

  # Time: O(n^2)
  # Space: O(n)

===

