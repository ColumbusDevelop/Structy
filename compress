def compress(s):
  
  s = s + "!"
  output = ""
  i = 0
  j = 0
  
  while j < len(s):
  
    if s[i] == s[j]:
      j = j + 1
    elif s[i] != s[j]:
      if j - i > 1:
        output = output + str(j - i) + s[i]
        i = j
      elif j - i == 1:
        output = output + s[i]
        i = j

  return output
